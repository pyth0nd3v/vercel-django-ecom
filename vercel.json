{
    "version": 2,
    "builds": [
      {
        "src": ".",
        "use": "@vercel/python"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "ECOM.wsgi:application"
      }
    ],
    "env": {
      "VERCEL_ENV": "production",
      "PYTHONUNBUFFERED": "1",
      "DJANGO_SETTINGS_MODULE": "ECOM.settings"
    }
  }
  